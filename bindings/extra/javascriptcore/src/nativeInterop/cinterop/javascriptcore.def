headers = jsc/jsc.h
package = org.gtkkn.native.javascriptcore
headerFilter = jsc/*.h

# pkg-config --cflags javascriptcoregtk-6.0 | tr ' ' '\n' | sed '$!s/$/ \\/'
compilerOpts.linux = \
    -I/usr/include/webkitgtk-6.0 \
    -I/usr/include/glib-2.0 \
    -I/usr/lib/glib-2.0/include \
    -I/usr/include/sysprof-6 \
    -pthread

compilerOpts.linux_x64 = \
    -I/usr/lib/x86_64-linux-gnu/glib-2.0/include \
    -I/usr/lib64/glib-2.0/include \

# pkg-config --libs javascriptcoregtk-6.0 | tr ' ' '\n' | sed '$!s/$/ \\/'
linkerOpts.linux = \
    -ljavascriptcoregtk-6.0 \
    -lgobject-2.0 \
    -lglib-2.0

strictEnums = \
    JSCCheckSyntaxMode \
    JSCCheckSyntaxResult \
    JSCOptionType \
    JSCTypedArrayType
